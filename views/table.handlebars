{{#each tables}}
  <h2>Table #{{this.dataValues.id}}</h2>

  {{#if this.dataValues.available}}
   
  <h3>Current Table Information:</h3>
  <p><strong>Name:</strong> {{this.dataValues.name}}</p>
  <p><strong>Phone:</strong> {{this.dataValues.phone}}</p>
  <p><strong>Email:</strong> {{this.dataValues.email}}</p>
  <p><strong>Party Size:</strong> {{this.dataValues.num_party}}</p>
  <p><strong>Reservation Time:</strong> {{this.dataValues.res_time}}</p>

  {{/if}}
{{/each}}
<a href="/admin/tables">Go Back</a>